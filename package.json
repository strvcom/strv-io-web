{
  "name": "strv-io-web",
  "description": "STRV.io website",
  "version": "1.0.0",
  "author": "STRV",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:ts": "eslint './**/*.{ts,tsx}' --fix",
    "lint:style": "stylelint './**/*.{ts,tsx}' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:ts",
      "npm run lint:style",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "^2.7.0",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-netlify-cms": "^4.1.6",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.7",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-json": "^2.2.2",
    "gatsby-transformer-sharp": "^2.2.3",
    "netlify-cms": "^2.9.6",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-img-webp": "^2.0.0",
    "react-reveal": "^1.2.2",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@strv/eslint-config-react": "^3.0.0",
    "@strv/eslint-config-typescript": "^2.0.0",
    "@strv/prettier-config": "^1.0.2",
    "@strv/stylelint-config-styled-components": "^1.1.0",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.6.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.6",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strvcom/strv-io-web"
  }
}
