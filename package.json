    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:ts",
      "npm run lint:style",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "^2.0.70",
    "gatsby-image": "^2.0.25",
    "gatsby-plugin-manifest": "^2.0.5",
